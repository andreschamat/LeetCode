class Solution:
    
    nums = [2,7,11,15]
    target = 9
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        index = 0
        length = len(nums)
        for int in "nums":
            if index  ==  length-1 :
                break
            firstint = nums[index]
            nextint = nums[index + 1]
            if firstint + nextint == target:
                answer = [index,index+1]
                return answer
                break
            index = index + 1
            
            
        
